{
  "name": "sso-auth-service",
  "version": "1.0.0",
  "description": "Authorization server for an SSO and OAuth2.0",
  "main": "app.mjs",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "confme": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "keycloak-connect": "^11.0.0",
    "livr": "^2.3.1",
    "lodash": "^4.17.15",
    "mariadb": "^2.3.1",
    "mysql2": "^2.1.0",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "sequelize": "^5.21.10",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --trace-uncaught app.mjs",
